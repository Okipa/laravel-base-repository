{
    "name": "okipa/laravel-base-repository",
    "description": "A configuration-based abstract repository with superpowers that simplifies your code.",
    "keywords": [
        "okipa",
        "php",
        "laravel",
        "base",
        "repository"
    ],
    "homepage": "https://github.com/Okipa/laravel-base-repository",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur LORENT",
            "email": "arthur.lorent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "~5.0",
        "intervention/image": "^2.4",
        "spatie/laravel-image-optimizer": "^1.1"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
            "Okipa\\LaravelBaseRepository\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okipa\\LaravelBaseRepository\\LaravelBaseRepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
